{
  "mcpServers": {
    "google-search": {
      "command": "npx",
      "args": ["@mcp-server/google-search-mcp@latest"],
      "env": {
        "GOOGLE_API_KEY": "",
        "GOOGLE_CSE_ID": ""
      }
    },
    "wikipedia": {
      "command": "npx",
      "args": ["wikipedia-mcp-server"]
    },
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest"],
      "env": { "DISPLAY": ":1" }
    },
    "airbnb": {
      "command": "npx",
      "args": ["@openbnb/mcp-server-airbnb"],
      "env": {
        "AIRBNB_API_KEY": ""
      }
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": { "GITHUB_PERSONAL_ACCESS_TOKEN": "" }
    },
    "git": {
      "command": "uvx",
      "args": ["mcp-server-git", "--repository", "C:/Users/debab/Desktop/IIT+SELF LEARNING/CODING/ModelContextProtocol"]
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "C:/Users/debab/Desktop"]
    },
    "postgres": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-postgres", "postgresql://user:pass@localhost/dbname"]
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"]
    }
  }
}
