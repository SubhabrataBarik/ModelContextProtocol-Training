{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest"],
      "env": {
        "DISPLAY": ":1"
      }
    },
    "airbnb": {
      "command": "npx",
      "args": [
        "-y",
        "@openbnb/mcp-server-airbnb"
      ]
    },
    "duckduckgo-search": {
      "command": "npx",
      "args": [
        "-y",
        "duckduckgo-mcp-server"
      ],
      "env": {
        "RATE_LIMIT_MS": "5000"
      }
    }
  }
}
